{
  "name": "tgapi",
  "version": "0.4.0",
  "description": "Actual Telegram bot API with Rx-driven updates and full Flow type coverage",
  "main": "lib/index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
  ],
  "scripts": {
    "lint": "eslint .",
    "flow": "flow",
    "test": "jest --env=node",
    "pretyped": "shx rm -rf flow-typed/npm",
    "typed": "flow-typed update",
    "posttyped": "flow-typed install jquery@3.2.1",
    "script": "NODE_ENV=script babel-node --",
    "tg-types": "yarn script src/scripts/generateTgTypes",
    "pretranspile": "yarn lint && yarn flow && yarn test",
    "transpile": "shx rm -rf lib && babel src --out-dir lib --ignore spec.js,test.js",
    "precommit": "yarn run pretranspile",
    "preversion": "git checkout master && yarn run build",
    "postversion": "npm publish && git push origin master --tags"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "form-data": "^2.3.1",
    "node-fetch": "^1.7.3",
    "ramda": "^0.24.1",
    "rxjs": "^5.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-ramda": "^1.4.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-flowtype": "^2.36.0",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.55.0",
    "flow-typed": "^2.1.5",
    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "nightmare": "^2.10.0",
    "shx": "^0.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigslycat/tgapi.git"
  },
  "keywords": [
    "API",
    "bot",
    "bots",
    "Telegram",
    "tg",
    "rx",
    "rxjs",
    "reactive",
    "flow",
    "flowtype"
  ],
  "author": {
    "name": "Pavel Tereschenko",
    "email": "me@pavlik.pro",
    "url": "https://github.com/bigslycat"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigslycat/tgapi/issues"
  },
  "homepage": "https://github.com/bigslycat/tgapi#readme"
}
