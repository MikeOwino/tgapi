{
  "name": "tgapi",
  "version": "1.0.7",
  "description": "Actual Telegram bot API with Rx-driven updates and full Flow type coverage",
  "main": "lib/bundle.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "lib",
    "src/util",
    "src/createBotClient.js",
    "src/createMiddleware.js",
    "src/createUpdateObserver.js",
    "src/createUpdateSubscription.js",
    "src/generatedTypes.js",
    "src/HTTPError.js",
    "src/index.js",
    "src/setWebhookToServer.js",
    "src/types.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "flow": "flow",
    "test": "jest --env=node",
    "pretyped": "shx rm -rf flow-typed/npm",
    "typed": "flow-typed update",
    "posttyped": "flow-typed install jquery@3.2.1",
    "script": "babel-node --",
    "tg-types": "yarn script src/scripts/generateTgTypes",
    "transpile": "shx rm -f lib/bundle.js && rollup -c",
    "precommit": "yarn lint && yarn flow && yarn test",
    "preversion": "git checkout master && yarn precommit",
    "postversion": "git push --follow-tags"
  },
  "dependencies": {
    "body-parser": "1.18.2",
    "form-data": "^2.3.1",
    "node-fetch": "^1.7.3",
    "ramda": "^0.25.0",
    "rxjs": "^5.5.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-ramda": "^1.4.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.12.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "flow-bin": "^0.60.1",
    "flow-typed": "^2.2.3",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "nightmare": "^2.10.0",
    "rollup": "^0.52.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "<=8.2.3 || ^8.2.7",
    "rollup-plugin-node-resolve": "3.0.0",
    "shx": "^0.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigslycat/tgapi.git"
  },
  "keywords": [
    "API",
    "bot",
    "bots",
    "Telegram",
    "tg",
    "rx",
    "rxjs",
    "reactive",
    "flow",
    "flowtype"
  ],
  "author": {
    "name": "Pavel Tereschenko",
    "email": "me@pavlik.pro",
    "url": "https://github.com/bigslycat"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigslycat/tgapi/issues"
  },
  "homepage": "https://github.com/bigslycat/tgapi#readme"
}
